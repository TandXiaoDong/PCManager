<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:ShikuIM.Styles" >


    <!--侧边按钮背景-->
    <Color x:Key="ShikuDarkBackground" >#35383E</Color>
    <SolidColorBrush x:Key="ShikuDarkBackgroundBrush" Color="{StaticResource ShikuDarkBackground}" />

    <!--气泡背景-->
    <Color x:Key="MyBubbleBackground">#eaeaea</Color>
    <SolidColorBrush x:Key="MyBubbleBackgroundBrush" Color="{StaticResource MyBubbleBackground}" />

    <!--几乎全黑-->
    <Color x:Key="TransDark" >#DE000000</Color>
    <SolidColorBrush x:Key="TransDarkBrush" Color="{StaticResource TransDark}" />


    <!--   
    <SolidColorBrush x:Key="ListItemBorderbrush" Color="#f7f7f7" />
    <SolidColorBrush x:Key="ShikuFlatIconColor" Color="#A8A8A8" />
    <SolidColorBrush x:Key="ShikuMDFontColor" Color="#222222" />
    -->


    <SolidColorBrush x:Key="TabItem.Selected.Background" Color="#FFFFFF" />
    <SolidColorBrush x:Key="TabItem.Selected.Border" Color="#ACACAC" />

    <SolidColorBrush x:Key="TabItem.MouseOver.Border" Color="#7EB4EA" />
    <SolidColorBrush x:Key="TabItem.Disabled.Background" Color="#F0F0F0" />
    <SolidColorBrush x:Key="TabItem.Disabled.Border" Color="#D9D9D9" />

</ResourceDictionary>
